# LLM Configuration
LLM_PROVIDER=openai  # Options: openai, ollama
OPENAI_API_KEY="Enter key here"
OPENAI_MODEL=gpt-4-turbo-preview  # Options: gpt-4-turbo-preview, gpt-3.5-turbo
OLLAMA_MODEL=llama2  # Used if LLM_PROVIDER=ollama

# Embedding Configuration
EMBEDDING_PROVIDER=openai  # Options: openai, sentence-transformers
EMBEDDING_MODEL=text-embedding-ada-002

# Vector Store
VECTOR_STORE_TYPE=faiss  # Options: faiss, chroma

# Processing Limits
MAX_FILE_SIZE_MB=10
